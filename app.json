{
  "expo": {
    "name": "jim",
    "slug": "jim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6F25CA"
    },
    "scheme": "jim",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yashwant.jim"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#6F25CA"
      },
      "package": "com.yashwant.jim"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8529d4b0-63d0-42a6-b473-898c03b8f9bf"
      }
    }
  }
}
